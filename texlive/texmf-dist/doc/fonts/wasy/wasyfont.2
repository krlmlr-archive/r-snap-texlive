\font\tenwasy = lasy10
\font\wasyb = lasyb10
\font\ninewasy = lasy9
\font\eightwasy = lasy8
\font\sevenwasy = lasy7
\font\fivewasy = lasy5
\newfam\wasyfam
\newcount\wasyfamcount
\wasyfamcount=\wasyfam \multiply\wasyfamcount by 256
\def\wasy{\fam\wasyfam\tenwasy}
