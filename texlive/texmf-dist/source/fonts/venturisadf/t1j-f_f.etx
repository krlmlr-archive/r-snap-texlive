\relax

% t1j-f_f.etx -- install a T1-encoded roman font with oldstyle 
% and f-ligatures named "f_f" etc.
%
% We do not need to distinguish between roman and italic in T1,
% hence we simply call t1-f_f.etx with oldstyle parameters.

\encoding

\setcommand\lc#1#2{#2}
\setcommand\uc#1#2{#1}
\setcommand\lctop#1#2{#2}
\setcommand\uctop#1#2{#1}
\setcommand\lclig#1#2{#2}
\setcommand\uclig#1#2{#1}
\setcommand\digit#1{#1oldstyle}

\inputetx{t1-f_f}

\endencoding
