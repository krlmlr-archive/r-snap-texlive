adjust_fit(0,0); pcshiftx:=u;
x1=w-pcshiftx-(0.5hstretch*dot_size); y1=h;
draw_pcomma(1,dot_size,comma_width);
penlabels(1); 
endchar; % end of "'"

%    \end{macrocode}
% \end{routine}
