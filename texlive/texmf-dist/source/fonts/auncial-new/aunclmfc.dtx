% left 0
lft x11=pcshiftx; y11=1/2h; x12=1/2w-1/2thick; y12=h;
draw_small_zero(11,12);
% right 0
x21=1/2w+1/2thick; y21=0; rt x22=w-pcshiftx; y22=1/2h;
draw_small_zero(21,22);
x1=1/2[x11,x12]; y1=0; x2=1/2[x21,x22]; y2=h;
