\newcommand*{\quotemarks@load@lang}[1]{%
\IfFileExists{quotmark-#1.def}{%
  \@ifundefined{quotemarks#1}{\input{quotmark-#1.def}}{}%
}{\PackageError{quotmark}{Language `#1' not implemented}{%
File `quotmark-#1.def' does not exist.}}%
}
