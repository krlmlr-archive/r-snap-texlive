%
%\begin{macro}{\ExecuteMetaData}
%    \begin{macrocode}
\newrobustcmd*\ExecuteMetaData[2][\jobname]{%
   \CatchFileBetweenTags\CatchFBT@tok{#1}{#2}%
   \global\expandafter\CatchFBT@tok\expandafter{%
            \expandafter}\the\CatchFBT@tok
}% \ExecuteMetaData
%    \end{macrocode}
% \end{macro}
