%Boisik driver file for text italic fonts
mode_setup; font_setup;

let bskboundary=endinput;
input bsklist-uc;
input bsklist-lc;
chars.sign_pound:=ASCII"$";

input bskletters-r;
input bskletters-i;
input bskletters-o;
input bsksymbols;

input bskligtab-i;

font_normal_space 5u#;
font_normal_stretch 3u#; font_normal_shrink 2u#;
font_quad 15u#;
 font_extra_space 2u#;
font_x_height 159/36pt#;
bye.
