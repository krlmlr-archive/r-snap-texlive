ligtable chars.greek_omega:          skewchar kern 1.5u#;
ligtable chars.greek_varsampi:       skewchar kern 2u#, tecarka -2u#, lom kern -.5u#;
ligtable chars.greek_qoppa:          skewchar kern 1u#;
ligtable chars.greek_stigma:         skewchar kern 1u#;

ligtable chars.math_ell:             skewchar kern 1.5u#, tecarka -u#;
