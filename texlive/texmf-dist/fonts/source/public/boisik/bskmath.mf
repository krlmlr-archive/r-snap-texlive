ligtable chars.math_partial:         skewchar kern 3u#, tecarka -u#;

ligtable chars.greek_Gamma:          skewchar kern 1u#, tecarka -3u#, lom kern -2u#;
ligtable chars.greek_Delta:          skewchar kern 3u#;
ligtable chars.greek_Theta:          skewchar kern 2u#, tecarka -1.5u#;
ligtable chars.greek_Lambda:         skewchar kern 3u#;
