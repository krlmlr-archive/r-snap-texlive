%Boisik driver file for math symbols and letters
mode_setup; font_setup;

let bskboundary=endinput;
numeric chars[];
input bsklist-mi;
input bsksymbols;  
input bskletters-o;
input bskletters-r;
input bskletters-i;

font_slant slant;

%Non-slanted characters
save chars; numeric chars[];
slant:=mono_charic#:=0;  
currenttransform:=identity yscaled aspect_ratio scaled granularity;
let bskboundary=relax;
input bsklist-mi-up;
input bsksymbols;

font_quad 15u#;
font_x_height 159/36pt#;

numeric skewchar, lom, carka, tecka; skewchar:=130; 
lom:=chars.math_virgule; carka:=chars.sign_comma; tecka:=chars.sign_period;
def tecarka text t:= tecka kern t, carka kern t enddef;
input bsklist-mi;

ligtable chars.latin_A:              skewchar kern 3u#;
ligtable chars.latin_B:              skewchar kern 1u#, tecarka -u#;
ligtable chars.latin_C:              skewchar kern 2u#, tecarka -u#;
ligtable chars.latin_D:              skewchar kern 1u#, tecarka -u#;
ligtable chars.latin_E:              skewchar kern 2u#;
ligtable chars.latin_F:              skewchar kern 2u#, tecarka -3u#, lom kern -2u#;
ligtable chars.latin_G:              skewchar kern 2u#;
ligtable chars.latin_H:              skewchar kern 1u#, tecarka -u#;
ligtable chars.latin_I:              skewchar kern 2.5u#, tecarka -u#;
ligtable chars.latin_J:              skewchar kern 3u#, tecarka -1.5u#, lom kern -u#;
ligtable chars.latin_K:              skewchar kern 1u#, tecarka -u#;
ligtable chars.latin_M:              skewchar kern 1.5u#, tecarka -u#;
ligtable chars.latin_N:              skewchar kern .5u#, tecarka -1.5u#, lom kern -.5u#;
ligtable chars.latin_O:              skewchar kern 2u#, tecarka -1.5u#;
ligtable chars.latin_P:              skewchar kern 1u#, tecarka -3u#, lom kern -u#;
ligtable chars.latin_Q:              skewchar kern 2u#, lom kern u#;
ligtable chars.latin_R:              skewchar kern 2u#;
ligtable chars.latin_S:              skewchar kern 2u#;
ligtable chars.latin_T:              skewchar kern 1u#, tecarka -3u#, lom kern -2u#;
ligtable chars.latin_U:              tecarka -1.5u#, lom kern -u#;
ligtable chars.latin_V:              tecarka -3u#, lom kern -3u#;
ligtable chars.latin_W:              tecarka -3u#, lom kern -2u#;
ligtable chars.latin_X:              skewchar kern 2u#, tecarka -u#;
ligtable chars.latin_Y:              skewchar kern 1u#, tecarka -3u#, lom kern -2u#;
ligtable chars.latin_Z:              skewchar kern 1u#, tecarka -u#;

ligtable chars.latin_a:              skewchar kern 2u#;
ligtable chars.latin_b:              tecarka -.5u#;
ligtable chars.latin_c:              skewchar kern 1.5u#;
ligtable chars.latin_d:              skewchar kern 2u#;
ligtable chars.latin_e:              skewchar kern 2u#;
ligtable chars.latin_f:              skewchar kern 4.5u#, tecarka -3u#, lom kern -2u#;
ligtable chars.latin_g:              skewchar kern u#, tecarka -u#;
ligtable chars.latin_g_alt:          skewchar kern 2u#;
ligtable chars.latin_h:              skewchar kern 2u#;
ligtable chars.latin_hstroke:        skewchar kern 2u#;
ligtable chars.latin_hbar:           skewchar kern 2u#;
ligtable chars.latin_i:              skewchar kern 1.5u#;
ligtable chars.latin_dotless_i:      skewchar kern 1u#;
