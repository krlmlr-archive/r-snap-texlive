ligtable chars.greek_Phi:            skewchar kern 1.5u#, tecarka -2u#;
ligtable chars.greek_Psi:            skewchar kern 1u#, tecarka -2u#;
ligtable chars.greek_Omega:          skewchar kern 2u#, tecarka -.5u#;
