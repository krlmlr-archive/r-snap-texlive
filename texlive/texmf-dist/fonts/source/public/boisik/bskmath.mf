ligtable chars.numeral_eight:        skewchar kern 2u#, tecarka -.5u#;
ligtable chars.numeral_nine:         skewchar kern 2u#, tecarka -.5u#;

ligtable lom:                        chars.latin_A kern -u#, chars.latin_J kern -u#, chars.latin_f kern -1.5u#, chars.greek_Delta kern -u#,
                                     chars.greek_Lambda kern -u#, chars.greek_Sigma kern -.5u#, chars.numeral_four kern -u#;
bye.
