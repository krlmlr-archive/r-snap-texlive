ligtable chars.latin_u:              skewchar kern 1u#;
ligtable chars.latin_v:              skewchar kern 1u#;
ligtable chars.latin_w:              skewchar kern 1u#;
ligtable chars.latin_x:              skewchar kern 1u#;
ligtable chars.latin_y:              skewchar kern 2.5u#;
ligtable chars.latin_z:              skewchar kern 1u#;

ligtable chars.greek_alpha:          skewchar kern 1.5u#;
