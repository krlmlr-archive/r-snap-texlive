% eiadbxsl10.mf: Irish Boldface Extended Slanted 10 point
if unknown cmbase: input cmbase fi
font_identifier:="EIADBXSL";
design_size := 10;
font_size design_size*pt#;
input c-cmbx;
slant:=1/6;
generate gaelach      % switch to the driver file
