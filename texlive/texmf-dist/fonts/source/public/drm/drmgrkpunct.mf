	shifted (0,-0.5pwid);

endchar;

beginchar(oct"020",1.75pwid#,cap#,0); "The open double quote, ``";

fill commapost rotatedaround ((w/2,h/2),180)
	shifted (0,-2dotwid);
fill commapost rotatedaround ((w/2,h/2),180)
	shifted (-0.75pwid,-2dotwid);

