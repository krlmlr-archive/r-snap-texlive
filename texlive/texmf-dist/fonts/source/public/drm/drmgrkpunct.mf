beginchar(oct"177",em#+2ss#,ex#,0); "The em-dash, ---";

z0 = (ss,3h/4);
z1 = (w-ss,y0);
z2 = z0 shifted (thinwid/2,thinwid/2);
z3 = z2 shifted (0,-thinwid);
z4 = z1 shifted (-thinwid/2,thinwid/2);
