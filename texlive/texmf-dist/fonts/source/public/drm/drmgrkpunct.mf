endchar;

beginchar(oct"021",1.75pwid#,cap#,0); "The close double quote, ''";

fill commapost shifted (0pwid,h-2dotwid);
fill commapost shifted (0.75pwid,h-2dotwid);

endchar;

beginchar(oct"255",1.75pwid#,cap#,0); "The baseline open double quote, ,,";

