penpos13(thickl,0);
z13l = (x12r,h/12);
penpos14(thickl,0);
z14r = (w-ss,0-o);

penstroke z0e..z1e..z2e..z3e..z4e--z5e;
penstroke z6e..z7e..z8e..z10e--z9e;
