penpos1(thinl,0);
z1l = (ss,3h/4);
penpos2(thickl,-90);
z2l = (x0,h+o);
penpos3(thickl,180);
z3 = (w/2,h/2);
penpos4(thickl,45);
x4 = x2; y4l = 0-o;
penpos5(thickl,45);
x5l = ss; y5r = h/4;
penpos6(thinl,180);
z6 = (x3,y1);
penpos7(1.5thinl,90);
z7r = (3w/4,h+o);
penpos8(thickl,0);
z8r = (w-ss,y6l);
z9r = z3l; z9l = z3r;
penpos10(thickl,0);
z10r = (w-ss,h/2+thickl);
penpos11(thickl,20);
z11l = z3;
penpos12(thickl,0);
